    "eslint": "^8.32.0",
    "husky": "^4.3.8",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.1.1",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "tscpaths": "^0.0.9",
