{
  "name": "extra-promise",
  "version": "0.4.3",
  "description": "Utilities for using Promise and Async/Await in JavaScript",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": "git@github.com:BlackGlory/extra-promise.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "precommit": "yarn lint && yarn test",
    "prepublishOnly": "rm -r lib && yarn build",
    "test": "jest --coverage",
    "build": "tsc --outDir lib",
    "lint": "tslint -t codeFrame 'src/**/*.ts' 'test/**/*.ts'"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "@types/node": "^12.7.8",
    "coveralls": "^3.0.2",
    "documentation": "^9.1.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typedoc": "^0.14.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {}
}
